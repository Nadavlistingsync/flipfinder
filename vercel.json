{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "trailingSlash": true,
  "cleanUrls": true,
  "framework": "nextjs",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "CHROME_EXECUTABLE_PATH": "/usr/bin/google-chrome"
  }
} 